    "@changesets/cli": "^2.28.1",
    "@cloudflare/vite-plugin": "0.1.19",
    "@cloudflare/workers-types": "^4.20250327.0",
    "@types/node": "^22.13.14",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "ai": "^4.2.8",
